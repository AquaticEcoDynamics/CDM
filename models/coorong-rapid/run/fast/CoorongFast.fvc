! Coorong Fast Mesh Test
!_________________________________________________________________

! GIS 
!_________________________________________________________________
GIS format == shp
shp projection == ../../model/fast/gis/shp/projection.prj


! TIME COMANDS
!_________________________________________________________________
time format == ISODATE
start time == 01/05/1998 00:00:00
end time == 01/03/2019 00:00:00

timestep limits == 0.1, 60.0
display dt == 7200.
cfl external == 1.0
cfl internal == 1.0


! SIMULATION CONFIGURATION
!_________________________________________________________________
include salinity == 1,1
include temperature == 1,1
spatial order == 1,1
horizontal gradient limiter == LCD
vertical gradient limiter == MC
bottom drag model == ks
equation of state == UNESCO


! MODEL PARAMETERS
!_________________________________________________________________
stability limits == 10.,10.
cell wet/dry depths == 1.0e-3, 1.0e-1
g == 9.81
reference density == 1025.
latitude == -35.8
Wind Stress Params == 0., 0.8E-03, 50., 4.05E-03


! HEAT PARAMETERS
!_________________________________________________________________
include heat == 1,0
longwave radiation model == 1
Shortwave radiation albedo == 0.05
Shortwave radiation fractions == 0.500, 0.463, 0.036, 0.001
Shortwave radiation extinction coefficients == 2.0, 1.2, 2.0, 2.5
Bulk Latent Heat Coefficient == 1.3e-3 ! Was 1.55e-3


! TURBULENCE PARAMETERS
!_________________________________________________________________
momentum mixing model == SMAGORINSKY
scalar mixing model == ELDER
vertical mixing model == external
external turbulence model dir == ../../external/GOTM/fast/
kinematic viscosity == 1.0e-6
global horizontal eddy viscosity == 0.2
global horizontal eddy viscosity limits == 1.0, 10.
global horizontal scalar diffusivity == 7000.0,700.0
global horizontal scalar diffusivity limits == 1.0, 50.0


! TRACER PARAMETERS
!_________________________________________________________________
Ntracer == 5
tracer == 1 ! Initial CSL passive tracer - initial concentration 100 in CSL
end tracer
tracer == 2 ! Ocean water passive tracer - concentration 100 at ocean boundaries including pump connections
end tracer
tracer == 3 ! Barrage flow passive tracer - concentration 100 in barrage flow
end tracer
tracer == 4 ! Salt Creek passive tracer - concentration 100 in salt creek flow
end tracer
tracer == 5 ! Water age passive tracer - initial concentration 0 and all inflow concentrations 0
  water age == 1
  evapo-concentrate == 0
end tracer


! GEOMETRY
!_________________________________________________________________
geometry 2d == ../../model/fast/geo/mesh/A10583_Coorong_fast_010.2dm
Cell elevation file == ../../model/fast/geo/cell_centres/Coorong_fast_010_cc_v07_MurrayMouth20171213_mean.csv, coordinate
echo geometry == 1

Read GIS Nodestring == ../../model/fast/gis/shp/2d_ns_Coorong_Fast_004_L.shp

Read GIS Mat == ../../model/fast/gis/shp/2d_mat_Coorong_Fast_003_R.shp
include == ../../model/fast/material/Coorong_materials_003.fvm


! STRUCTURE SPECIFICATIONS
!_________________________________________________________________
structure == zone, 1
	polygon file == ../../model/fast/structure/zone_poly_Coorong_HD_006.csv
	bed adjust == bathy_database
	bathy database == ../../model/fast/structure/bathy_database_drought.csv
	update dt == 1. ! hrs
	
	control == timeseries
		control parameter == bathy_control
		control header == Tauwitchere_Flow_m3_s
		control update dt == 168. ! hrs
		start control state == 0.
		response parameters == 336, 2016. ! hrs
		control file == ../../bc_dbase/calibration/inflow/Barrage_BC_19900101_20210531.csv
	end control	
end structure


! BOUNDARY CONDITIONS
!_________________________________________________________________
! Tide
include == ../../bc_dbase/calibration/include/tide_boundary_003_19900101_20200731.fvc

! Inflows
include == ../../bc_dbase/fast/include/inflow_Salt_Creek_003_19900101_20210531_fast.fvc
include == ../../bc_dbase/calibration/include/inflow_barrages_001_19900101_20210531.fvc

! Atmospherics
include == ../../bc_dbase/fast/include/BARRA_Parnaka_Pt_wind_19900101_20190301.fvc
include == ../../bc_dbase/fast/include/BARRA_Parnaka_Pt_temp_19900101_20190301.fvc
include == ../../bc_dbase/fast/include/BARRA_Parnaka_Pt_rad_SW_19900101_20190301.fvc
include == ../../bc_dbase/fast/include/BARRA_Parnaka_Pt_rad_LW_19900101_20190301.fvc

! SILO Met BCs
include == ../../bc_dbase/calibration/include/SILO_rel_hum_19900101_20210625.fvc
include == ../../bc_dbase/calibration/include/SILO_precip_005_19900101_20210625.fvc


! INITIAL CONDITIONS
!_________________________________________________________________
Initial Condition 2D == ../../bc_dbase/fast/initial/IC_Coorong_19980501__A10583_Coorong_fast_010_2013scale.csv


! OUTPUT COMMANDS
!_________________________________________________________________

!write check files  == /scratch2/A10583/TFV/check/

output dir == ../../output
!output dir == /scratch2/A10583/TFV/output/

output == flux
	output interval == 7200
end output

output == netcdf
	output parameters == h,v,d,Sal,Temp,trace_1,trace_2,trace_3,trace_4,trace_5,zb
!	output parameters == h,v,zb,Sal,Temp
	output interval == 86400
end output

Output == points
	Output points file == ../../model/points/output_points_001.csv
	Output parameters == h,sal,temp,evap,precip,trace_1,trace_2,trace_3,trace_4,trace_5
	Output interval == 7200 !Need 2 hrly output to do DTR
end output

